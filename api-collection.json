		{
			"name": "Negotiation",
			"description": "Capability assessment endpoint for task negotiation.",
			"item": [
				{
					"name": "Assess Task Capability",
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"task_summary\": \"Answer questions about machine learning concepts\",\n  \"task_details\": \"User needs help understanding neural networks and deep learning\",\n  \"input_mime_types\": [\"text/plain\", \"application/json\"],\n  \"output_mime_types\": [\"application/json\"],\n  \"max_latency_ms\": 5000,\n  \"max_cost_amount\": 0.01,\n  \"required_tools\": [],\n  \"forbidden_tools\": [],\n  \"min_score\": 0.5,\n  \"weights\": {\n    \"skill_match\": 0.55,\n    \"io_compatibility\": 0.20,\n    \"performance\": 0.15,\n    \"load\": 0.05,\n    \"cost\": 0.05\n  }\n}"
						},
						"url": {
							"raw": "{{base_url}}/agent/negotiation",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"agent",
								"negotiation"
							]
						},
						"description": "Assess agent's capability to handle a task. Evaluates skill match, IO compatibility, performance, load, and cost to produce an acceptance decision with confidence and detailed scoring.\n\n**Required fields:**\n- task_summary (string, max 10000 chars)\n\n**Optional fields:**\n- task_details, input_mime_types, output_mime_types, max_latency_ms, max_cost_amount, required_tools, forbidden_tools, min_score, weights"
					}
				}
			]
		},
		{
			"name": "Payment",
			"description": "x402 payment protocol endpoints for paid agent services.",
			"item": [
				{
					"name": "Start Payment Session",
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Authorization",
								"value": "Bearer {{api_key}}"
							}
						],
						"url": {
							"raw": "{{base_url}}/api/start-payment-session",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"api",
								"start-payment-session"
							]
						},
						"description": "Initiates a payment session for x402 protocol. Returns payment requirements, session ID, browser URL for payment capture, and expiration time."
					}
				},
				{
					"name": "Get Payment Status",
					"request": {
						"method": "GET",
						"header": [
							{
								"key": "Authorization",
								"value": "Bearer {{api_key}}"
							}
						],
						"url": {
							"raw": "{{base_url}}/api/payment-status/:sessionId?wait=false",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"api",
								"payment-status",
								":sessionId"
							],
							"query": [
								{
									"key": "wait",
									"value": "false",
									"description": "Set to 'true' to wait for completion (up to 5 minutes)"
								}
							],
							"variable": [
								{
									"key": "sessionId",
									"value": "",
									"description": "The payment session identifier"
								}
							]
						},
						"description": "Retrieves the status of a payment session. Used to verify payment completion. The payment token is returned but NOT consumed - it can be used for the actual API call.\n\n**Query Parameters:**\n- wait: Set to 'true' to wait for completion (up to 5 minutes)"
					}
				},
				{
					"name": "Payment Capture Page",
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "{{base_url}}/payment-capture?session_id=",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"payment-capture"
							],
							"query": [
								{
									"key": "session_id",
									"value": "",
									"description": "The payment session ID"
								}
							]
						},
						"description": "Browser page to capture payment. Shows paywall UI and captures payment token when completed. This is typically opened in a browser, not called programmatically."
					}
				}
			]
		},
