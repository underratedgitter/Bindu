		{
			"name": "DID Resolution",
			"description": "Decentralized Identifier resolution endpoints.",
			"item": [
				{
					"name": "Resolve DID (POST)",
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"did\": \"did:bindu:raahul_at_getbindu_com:first_agent:e091ddb0b2c84bd8991fdfd1ecbb9da6\"\n}"
						},
						"url": {
							"raw": "{{base_url}}/did/resolve",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"did",
								"resolve"
							]
						},
						"description": "Resolves a Decentralized Identifier (DID) to its W3C-compliant DID document. Supports custom Bindu DID format: did:bindu:{author}:{agent_name}:{agent_id}. No authentication required."
					}
				},
				{
					"name": "Resolve DID (GET)",
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "{{base_url}}/did/resolve?did=did:bindu:raahul_at_getbindu_com:first_agent:e091ddb0b2c84bd8991fdfd1ecbb9da6",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"did",
								"resolve"
							],
							"query": [
								{
									"key": "did",
									"value": "did:bindu:raahul_at_getbindu_com:first_agent:e091ddb0b2c84bd8991fdfd1ecbb9da6",
									"description": "The DID to resolve"
								}
							]
						},
						"description": "Resolves a Decentralized Identifier (DID) via GET request with query parameter."
					}
				}
			]
		},
